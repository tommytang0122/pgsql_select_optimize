# Application Settings
DEBUG=false
CORS_ORIGINS=http://localhost:8000

# Rate Limiting (requests per minute)
RATE_LIMIT_DATA_ALL=5
RATE_LIMIT_DATA=30
RATE_LIMIT_DEFAULT=60
